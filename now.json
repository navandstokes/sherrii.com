{
	"version": 2,
	"name": "sherrii",
	"builds": [
		{ "src": "package.json", "use": "@now/next" }
	],
	"routes": [
		{ "src": "/about", "dest": "/about"},
		{ "src": "/p/(?<slug>[^/]+)$", "dest": "/page?slug=$slug" },
		{ "src": "/(?<slug>[^/]+)$", "dest": "/main?slug=$slug" },
		{ "src": "/", "dest": "/main?slug=portraits"},
		{ "src": "/(.*)", "dest": "/$1" }
	]
}