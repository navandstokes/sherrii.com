{
	"version": 2,
	"trailingSlash": false,
	"rewrites": [
		{"source": "/", "destination": "/portraits"}
	],
	"headers": [
	  {
	    "source": "/(.*)",
	    "headers": [
	      {
	        "key": "Cache-Control",
	        "value": "s-maxage=600, stale-while-revalidate"
	      }
	    ]
	  }
	],
	"builds": [{ "src": "package.json", "use": "@now/next@2.3.1" }]
}